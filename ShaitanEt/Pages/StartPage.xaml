<Page x:Class="ShaitanEt.Pages.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ShaitanEt.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="900" d:DesignWidth="1200"
      Title="StartPage">

    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="AuthBtn" Width="300" Height="70" Content="АВТОРИЗАЦИЯ" Margin="30" Click="AuthBtn_Click"></Button>
            <Button x:Name="RegBtn" Width="300" Height="70" Content="РЕГИСТРАЦИЯ" Click="RegBtn_Click"></Button>
        </StackPanel>
    </Grid>
</Page>
